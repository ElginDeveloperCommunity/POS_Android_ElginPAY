<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="376dp"
        android:layout_height="196dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="112dp">

        <Button
            android:id="@+id/buttonDebito"
            android:layout_width="372dp"
            android:layout_height="wrap_content"
            android:text="Iniciar Pagamento Débito" />

        <Button
            android:id="@+id/buttonCredito"
            android:layout_width="371dp"
            android:layout_height="wrap_content"
            android:text="Iniciar Pagamento Crédito" />

        <Button
            android:id="@+id/buttonCancelamento"
            android:layout_width="372dp"
            android:layout_height="wrap_content"
            android:text="Iniciar Cancelamento Venda " />

        <Button
            android:id="@+id/buttonAdministrativa"
            android:layout_width="373dp"
            android:layout_height="wrap_content"
            android:text="Iniciar Operação Administrativa" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>